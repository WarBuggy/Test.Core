@page
@using Microsoft.AspNetCore.Mvc.Localization
@using Test.Core.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@using Test.Core.Web.Pages.Distributors
@using Test.Core.Distributors;
@using System.Globalization
@inject IHtmlLocalizer<CoreResource> L
@model CreateModalModel
@{
    Layout = null;
}

<form data-ajaxForm="true" asp-page="/Distributors/CreateModal" autocomplete="off">
    <abp-modal id="DistributorCreateModal">
        <abp-modal-header title="@L["NewDistributor"].Value"></abp-modal-header>

        <abp-modal-body>
                    
            <abp-input asp-for="Distributor.CompanyName"/>

            <abp-input asp-for="Distributor.TaxID"/>

                    
                
        </abp-modal-body>

        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</form>